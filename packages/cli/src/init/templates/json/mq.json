{
  "redis": {
    "label": "Redis",
    "packageManagers": [
      "deno",
      "bun",
      "npm",
      "yarn",
      "pnpm"
    ],
    "dependencies": {
      "npm:ioredis": "^5.4.1"
    },
    "imports": {
      "@fedify/redis": {
        "RedisMessageQueue": "RedisMessageQueue"
      },
      "ioredis": {
        "Redis": "Redis"
      }
    },
    "object": "new RedisMessageQueue(() => new Redis(process.env.REDIS_URL))",
    "env": {
      "REDIS_URL": "redis://localhost:6379"
    }
  },
  "postgres": {
    "label": "PostgreSQL",
    "packageManagers": [
      "deno",
      "bun",
      "npm",
      "yarn",
      "pnpm"
    ],
    "dependencies": {
      "npm:postgres": "^3.4.5"
    },
    "imports": {
      "@fedify/postgres": {
        "PostgresMessageQueue": "PostgresMessageQueue"
      },
      "postgres": {
        "default": "postgres"
      }
    },
    "object": "new PostgresMessageQueue(postgres(process.env.DATABASE_URL))",
    "env": {
      "DATABASE_URL": "postgres://postgres@localhost:5432/postgres"
    }
  },
  "amqp": {
    "label": "AMQP (e.g., RabbitMQ)",
    "packageManagers": [
      "deno",
      "bun",
      "npm",
      "yarn",
      "pnpm"
    ],
    "dependencies": {
      "npm:amqplib": "^0.10.4"
    },
    "devDependencies": {
      "npm:@types/amqplib": "^0.10.5"
    },
    "imports": {
      "@fedify/amqp": {
        "AmqpMessageQueue": "AmqpMessageQueue"
      },
      "amqplib": {
        "connect": "connect"
      }
    },
    "object": "new AmqpMessageQueue(await connect(process.env.AMQP_URL))",
    "env": {
      "AMQP_URL": "amqp://localhost"
    }
  },
  "denokv": {
    "label": "Deno KV",
    "packageManagers": [
      "deno"
    ],
    "imports": {
      "@fedify/denokv": {
        "DenoKvMessageQueue": "DenoKvMessageQueue"
      }
    },
    "object": "new DenoKvMessageQueue(await Deno.openKv())",
    "denoUnstable": [
      "kv"
    ]
  }
}
